{
  "name": "forge-dev",
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fetch-ponyfill": "^7.1.0",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/node-fetch": "^2.5.8",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.3",
    "typescript-is": "^0.17.0",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0"
  },
  "scripts": {
    "build:client": "env-cmd -f .env webpack --color --progress -c ./webpack.client.config.js",
    "build:client:watch": "env-cmd -f .env webpack --color --progress --watch  -c ./webpack.client.config.js",
    "build:server": "env-cmd -f .env webpack --color --progress -c ./webpack.server.config.js",
    "build:server:watch": "env-cmd -f .env webpack --color --progress --watch -c ./webpack.server.config.js",
    "build": "npm-run-all -p build:client build:server",
    "build:watch": "npm-run-all -p build:client:watch build:server:watch",
    "lint": "eslint . --ext .tsx --ext .ts",
    "fmt": "npm run lint -- --fix",
    "start:server": "env-cmd -f .env node ./dist/app.js",
    "start:server:watch": "env-cmd -f .env nodemon --watch ./dist/app.js ./dist/app.js",
    "dev": "npm-run-all -p build:client:watch build:server:watch start:server:watch"
  }
}